  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathantneal/postcss-time-machine.git"
  },
  "homepage": "https://github.com/jonathantneal/postcss-time-machine",
