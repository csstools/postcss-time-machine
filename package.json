{
  "name": "postcss-time-machine",
  "version": "1.2.1",
  "description": "Fix mistakes in the design of CSS itself",
  "main": "index.js",
  "dependencies": {
    "postcss": "^5.0.14",
    "postcss-selector-parser": "^1.3.1",
    "postcss-value-parser": "^3.2.3"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  },
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "tape": "tape test.js | tap-spec",
